<?xml version="1.0" encoding="UTF-8"?>
<com.sglabs.medistant.elements.accountactivity.AccountLogin xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/account_login"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFFFF">

    <android.support.v7.widget.AppCompatEditText
        android:id="@+id/login_email"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ellipsize="end"
        android:focusableInTouchMode="true"
        android:fontFamily="sans-serif"
        android:gravity="center|start"
        android:hint="@string/login_email_hint"
        android:inputType="textEmailAddress"
        android:letterSpacing="0"
        android:lineSpacingMultiplier="1"
        android:maxLines="1"
        android:text="@string/login_email_text"
        android:textColor="#FF000000"
        android:textColorHint="#FF009688"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_heightPercent="9.000000357628%"
        app:layout_marginStartPercent="15.000000596046%"
        app:layout_marginTopPercent="5.000000074506%"
        app:layout_widthPercent="69.999998807907%" />

    <android.support.v7.widget.AppCompatEditText
        android:id="@+id/login_pw"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ellipsize="end"
        android:focusableInTouchMode="true"
        android:fontFamily="sans-serif"
        android:gravity="center|start"
        android:hint="@string/login_pw_hint"
        android:inputType="textPassword"
        android:letterSpacing="0"
        android:lineSpacingMultiplier="1"
        android:maxLines="1"
        android:text="@string/login_pw_text"
        android:textColor="#FF000000"
        android:textColorHint="#FF009688"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_heightPercent="9.000000357628%"
        app:layout_marginStartPercent="15.000000596046%"
        app:layout_marginTopPercent="20.000000298023%"
        app:layout_widthPercent="69.999998807907%" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/login_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#FF009688"
        android:cropToPadding="true"
        android:fontFamily="sans-serif"
        android:gravity="center|center"
        android:letterSpacing="0"
        android:text="@string/login_btn_text"
        android:textColor="#FFFFFFFF"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_heightPercent="7.999999821186%"
        app:layout_marginStartPercent="15.000000596046%"
        app:layout_marginTopPercent="34.999999403954%"
        app:layout_widthPercent="69.999998807907%" />
</com.sglabs.medistant.elements.accountactivity.AccountLogin>
