<?xml version="1.0" encoding="UTF-8"?>
<com.sglabs.medistant.elements.accountactivity.AccountRegister xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/account_register"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFFFF">

    <android.support.v7.widget.AppCompatEditText
        android:id="@+id/register_name"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ellipsize="end"
        android:focusableInTouchMode="true"
        android:fontFamily="sans-serif"
        android:gravity="center|start"
        android:hint="@string/register_name_hint"
        android:inputType="text"
        android:letterSpacing="0"
        android:lineSpacingMultiplier="1"
        android:maxLines="1"
        android:text="@string/register_name_text"
        android:textColor="#FF000000"
        android:textColorHint="#FF009688"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_heightPercent="9.000000357628%"
        app:layout_marginStartPercent="15.000000596046%"
        app:layout_marginTopPercent="5.000000074506%"
        app:layout_widthPercent="69.999998807907%" />

    <android.support.v7.widget.AppCompatEditText
        android:id="@+id/register_email"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ellipsize="end"
        android:focusableInTouchMode="true"
        android:fontFamily="sans-serif"
        android:gravity="center|start"
        android:hint="@string/register_email_hint"
        android:inputType="textEmailAddress"
        android:letterSpacing="0"
        android:lineSpacingMultiplier="1"
        android:maxLines="1"
        android:text="@string/register_email_text"
        android:textColor="#FF000000"
        android:textColorHint="#FF009688"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_heightPercent="9.000000357628%"
        app:layout_marginStartPercent="15.000000596046%"
        app:layout_marginTopPercent="20.000000298023%"
        app:layout_widthPercent="69.999998807907%" />

    <android.support.v7.widget.AppCompatEditText
        android:id="@+id/register_pw"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ellipsize="end"
        android:focusableInTouchMode="true"
        android:fontFamily="sans-serif"
        android:gravity="center|start"
        android:hint="@string/register_pw_hint"
        android:inputType="textPassword"
        android:letterSpacing="0"
        android:lineSpacingMultiplier="1"
        android:maxLines="1"
        android:text="@string/register_pw_text"
        android:textColor="#FF000000"
        android:textColorHint="#FF009688"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_heightPercent="9.000000357628%"
        app:layout_marginStartPercent="15.000000596046%"
        app:layout_marginTopPercent="34.999999403954%"
        app:layout_widthPercent="69.999998807907%" />

    <android.support.v7.widget.AppCompatEditText
        android:id="@+id/register_confpw"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ellipsize="end"
        android:focusableInTouchMode="true"
        android:fontFamily="sans-serif"
        android:gravity="center|start"
        android:hint="@string/register_confpw_hint"
        android:inputType="textPassword"
        android:letterSpacing="0"
        android:lineSpacingMultiplier="1"
        android:maxLines="1"
        android:text="@string/register_confpw_text"
        android:textColor="#FF000000"
        android:textColorHint="#FF009688"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_heightPercent="9.000000357628%"
        app:layout_marginStartPercent="15.000000596046%"
        app:layout_marginTopPercent="50%"
        app:layout_widthPercent="69.999998807907%" />

    <android.support.v7.widget.AppCompatEditText
        android:id="@+id/register_dob"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ellipsize="end"
        android:focusableInTouchMode="true"
        android:fontFamily="sans-serif"
        android:gravity="center|start"
        android:hint="@string/register_dob_hint"
        android:inputType="text"
        android:letterSpacing="0"
        android:lineSpacingMultiplier="1"
        android:maxLines="1"
        android:text="@string/register_dob_text"
        android:textColor="#FF000000"
        android:textColorHint="#FF009688"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_heightPercent="9.000000357628%"
        app:layout_marginStartPercent="15.000000596046%"
        app:layout_marginTopPercent="64.999997615814%"
        app:layout_widthPercent="69.999998807907%" />

    <com.sglabs.medistant.elements.accountactivity.RegisterSex
        android:id="@+id/register_sex"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:prompt="@string/register_sex_prompt"
        android:spinnerMode="dialog"
        app:layout_marginStartPercent="15.000000596046%"
        app:layout_marginTopPercent="80.000001192093%"
        app:layout_widthPercent="69.999998807907%" />

    <android.support.v7.widget.AppCompatCheckBox
        android:id="@+id/blood_donate_checkbox"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="#FFFFFFFF"
        android:buttonTint="#FF009688"
        android:checked="false"
        android:cropToPadding="true"
        android:fontFamily="sans-serif"
        android:gravity="center|start"
        android:letterSpacing="0"
        android:text="@string/blood_donate_checkbox_text"
        android:textColor="#FF009688"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_marginStartPercent="15.000000596046%"
        app:layout_marginTopPercent="87.999999523163%"
        app:layout_widthPercent="69.999998807907%" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/register_submit"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#FF009688"
        android:cropToPadding="true"
        android:fontFamily="sans-serif"
        android:gravity="center|center"
        android:letterSpacing="0"
        android:text="@string/register_submit_text"
        android:textColor="#FFFFFFFF"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_heightPercent="7.999999821186%"
        app:layout_marginStartPercent="15.000000596046%"
        app:layout_marginTopPercent="100%"
        app:layout_widthPercent="69.999998807907%" />
</com.sglabs.medistant.elements.accountactivity.AccountRegister>
